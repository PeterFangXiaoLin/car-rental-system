<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.mapper.StoreMapper">

    <resultMap id="BaseResultMap" type="com.my.domain.entity.Store">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="storeName" column="storeName" jdbcType="VARCHAR"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="longitude" column="longitude" jdbcType="DECIMAL"/>
            <result property="latitude" column="latitude" jdbcType="DECIMAL"/>
            <result property="mobile" column="mobile" jdbcType="VARCHAR"/>
            <result property="openTime" column="openTime" jdbcType="TIME"/>
            <result property="closeTime" column="closeTime" jdbcType="TIME"/>
            <result property="status" column="status" jdbcType="TINYINT"/>
            <result property="images" column="images" jdbcType="VARCHAR"/>
            <result property="createTime" column="createTime" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="updateTime" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="isDelete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,storeName,address,
        longitude,latitude,mobile,
        openTime,closeTime,status,
        images,createTime,updateTime,
        isDelete
    </sql>
</mapper>
